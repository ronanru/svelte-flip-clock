{
  "name": "svelte-flip-clock",
  "description": "A flip clock component for Svelte",
  "version": "0.0.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ronanru/svelte-flip-clock.git"
  },
  "homepage": "https://svelte-flip-clock.ronanru.dev",
  "bugs": "https://github.com/ronanru/svelte-flip-clock/issues",
  "author": {
    "name": "Matvey Ryabchikov",
    "url": "https://ronanru.com"
  },
  "scripts": {
    "dev": "vite dev",
